<%- include("../partials/header") %>
<h1>Edit Patient Note</h1>

<form action="/patients/<%= patient.id %>/<%= note.id %>?_method=PUT" method="POST" autocomplete="off">
    <input type="text" name="noteTitle" value="<%= note.noteTitle %>"> 
    <input type="text" name="noteContent" value="<%= note.noteContent %>">
    <input type="text" name="noteDate" value="<%= note.noteDate %>">
    <button type="button" class="btn btn-secondary" id="update-note">Update Patient Note</button>
</form>


<%- include("../partials/footer") %>